#ifndef ARDUINO_H_
#define ARDUINO_H_

typedef struct {
	uint32_t BaseAddress;	/* Device base address */
//	u32 IsReady;		/* Device is initialized and ready */
	uint32_t InterruptPresent;	/* Are interrupts supported in h/w */
//	int IsDual;		/* Are 2 channels supported in h/w */
} arduino_t;

#endif /* ARDUINO_H_ */
